internal class Program
{
    private static void Main(string[] args)
    {
        Random random = new Random();
        int number = random.Next(1, 101);
        int sum = 0;

        for (int i = 1; i <= number; i++)
        {
            if (i % 3 == 0 || i % 5 == 0)
                sum += i;
        }
        Console.WriteLine(sum);
    }
}
